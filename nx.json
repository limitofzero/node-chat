{
  "npmScope": "messenger",
  "affected": { "defaultBase": "master" },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": { "cacheableOperations": ["build", "lint", "test", "e2e"] }
    }
  },
  "projects": {
    "messenger-app": { "tags": [] },
    "messenger-app-e2e": {
      "tags": [],
      "implicitDependencies": ["messenger-app"]
    },
    "api": { "tags": [] },
    "api-interfaces": { "tags": [] },
    "dto": { "tags": [] },
    "api-errors": { "tags": [] },
    "common": { "tags": [] },
    "ui": { "tags": [] },
    "user-svc": { "tags": [] }
  }
}
